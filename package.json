    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "lerna": "^3.13.4",
    "lerna-changelog": "^0.8.2",
    "meow": "^5.0.0",
    "multimatch": "^4.0.0",
    "prettier": "^1.17.0",
    "puppeteer": "^1.15.0",
    "strip-ansi": "^5.2.0",
    "tempy": "^0.3.0"
  }
